\begin_layout Section
\begin_inset CommandInset label
LatexCommand label
name "sec:Integrated-moments-Heston"

\end_inset

Integrated moments for Heston model
\end_layout

\begin_layout Standard
The spot volatility equation can be integrated on the interval 
\begin_inset Formula $\left[t,t+h\right]$
\end_inset

 with 
\begin_inset Formula $h$
\end_inset

 possibly different from 
\begin_inset Formula $H$
\end_inset

 to get 
\begin_inset Formula 
\begin{equation}
\sigma_{t+h}^{2}=C_{h}+A_{h}\sigma_{t}^{2}+\epsilon_{t,h}^{\sigma},\label{eq:spot_vol_equation}
\end{equation}

\end_inset

where I define coefficients and the integrated innovation as
\begin_inset Formula 
\[
A_{h}=\exp\left(-\kappa h\right),\quad C_{h}=\mu\left(1-A_{h}\right),\quad\epsilon_{t,h}^{\sigma}=\eta_{\sigma}\int_{t}^{t+h}\sigma_{u}A_{t+h-u}dW_{u}^{\sigma}.
\]

\end_inset


\end_layout

\begin_layout Standard
The spot volatility first moment is
\begin_inset Formula 
\[
E_{t}\left[\sigma_{t+h}^{2}\right]=C_{h}+A_{h}\sigma_{t}^{2},
\]

\end_inset

or, using lag operator 
\begin_inset Formula $L$
\end_inset

,
\begin_inset Formula 
\[
E_{t}\left[\left(1-A_{h}L\right)\sigma_{t+h}^{2}\right]=\left(1-A_{h}\right)E\left[\sigma_{t}^{2}\right]=\left(1-A_{h}\right)\mu.
\]

\end_inset


\end_layout

\begin_layout Standard
Given the above definitions it is also not hard to guess that 
\begin_inset Formula $\sigma_{t}^{4}$
\end_inset

 is autoregressive:
\begin_inset Formula 
\[
E_{t}\left[\left(1-A_{h}L\right)\left(1-A_{h}^{2}L\right)\sigma_{t+2h}^{4}\right]=C_{1}=\left(1-A_{h}\right)\left(1-A_{h}^{2}\right)E\left[\sigma_{t}^{4}\right]
\]

\end_inset

with
\begin_inset Formula 
\[
E\left[\sigma_{t}^{4}\right]=V\left[\sigma_{t}^{2}\right]+\left(E\left[\sigma_{t}^{2}\right]\right)^{2}=\frac{\mu\eta^{2}}{2\kappa}+\mu^{2}.
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Note Comment
status open

\begin_layout Plain Layout
The conditional variance of the spot volatility is
\begin_inset Formula 
\[
\begin{aligned}V_{t}\left[\sigma_{t+h}^{2}\right]= & V_{t}\left[\epsilon_{t,h}^{\sigma}\right]\\
= & \eta^{2}\int_{t}^{t+h}E_{t}\left[\sigma_{u}^{2}\right]A_{t+h-u}^{2}du\\
= & \eta^{2}\int_{t}^{t+h}\left(A_{u-t}\sigma_{t}^{2}+C_{u-t}\right)A_{t+h-u}^{2}du\\
= & \sigma_{t}^{2}\eta^{2}\int_{0}^{h}A_{u}A_{h-u}^{2}du+\eta^{2}\int_{0}^{h}C_{u}A_{h-u}^{2}du\\
= & \sigma_{t}^{2}\eta^{2}\int_{0}^{h}A_{2h-u}du+\eta^{2}\int_{0}^{h}\mu\left(A_{2h-2u}-A_{3h-2u}\right)du\\
= & D_{1}\sigma_{t}^{2}+F_{1},
\end{aligned}
\]

\end_inset

with
\begin_inset Formula 
\[
\begin{aligned}D_{1}= & \eta^{2}\int_{0}^{h}A_{u}A_{h-u}^{2}du=\eta^{2}\int_{0}^{h}A_{2h-u}du=\frac{\eta^{2}}{\kappa}A_{h}\left(1-A_{h}\right),\\
F_{1}= & \eta^{2}\int_{0}^{h}C_{u}A_{h-u}^{2}du=\mu\eta^{2}\int_{0}^{h}\left(A_{2h-2u}-A_{3h-2u}\right)du=\frac{\mu\eta^{2}}{2\kappa}\left(1-A_{2h}\right)\left(1-A_{h}\right).
\end{aligned}
\]

\end_inset

Hence, the second moment of the spot volatility is
\begin_inset Formula 
\[
E_{t}\left[\sigma_{t+h}^{4}\right]=F_{1}+C_{h}^{2}+\left(D_{1}+2A_{h}C_{h}\right)\sigma_{t}^{2}+A_{h}^{2}\sigma_{t}^{4}.
\]

\end_inset

Using the lag operator we can make rewrite this equation as
\begin_inset Formula 
\[
E_{t}\left[\left(1-A_{h}^{2}L\right)\sigma_{t+h}^{4}\right]=F_{1}+C_{h}^{2}+\left(D_{1}+2A_{h}C_{h}\right)\sigma_{t}^{2}.
\]

\end_inset

Multiplying this equation by 
\begin_inset Formula $\left(1-A_{h}L\right)$
\end_inset

 and moving the reference point by 
\begin_inset Formula $h$
\end_inset

 we obtain
\begin_inset Formula 
\[
E_{t}\left[\left(1-A_{h}L\right)\left(1-A_{h}^{2}L\right)\sigma_{t+2h}^{4}\right]=\left(1-A_{h}\right)\left(F_{1}+C_{h}^{2}\right)+\left(D_{1}+2A_{h}C_{h}\right)C_{h}\equiv R_{1}.
\]

\end_inset


\end_layout

\end_inset


\end_layout

\begin_layout Standard
After repeated integration of the volatility equation 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:spot_vol_equation"

\end_inset

 over 
\begin_inset Formula $t$
\end_inset

 as a dummy of the integration on the interval 
\begin_inset Formula $\left[t,t+H\right]$
\end_inset

 we obtain
\begin_inset Formula 
\[
\mathcal{V}_{t+h,H}=C_{h}+A_{h}\mathcal{V}_{t,H}+\frac{1}{H}\int_{0}^{H}\epsilon_{t+s,h}^{\sigma}ds.
\]

\end_inset

Hence, the integrated volatility first moment is
\begin_inset Formula 
\[
E_{t}\left[\left(1-A_{h}L\right)\mathcal{V}_{t+h,H}\right]=\left(1-A_{h}\right)E\left[\mathcal{V}_{t,H}\right]=\left(1-A_{h}\right)\mu.
\]

\end_inset


\end_layout

\begin_layout Standard
We can also integrate the volatility equation 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:spot_vol_equation"

\end_inset

 using 
\begin_inset Formula $h$
\end_inset

 as a dummy of integration on the interval from 
\begin_inset Formula $\left[0,H\right]$
\end_inset

: 
\begin_inset Formula 
\begin{equation}
\mathcal{V}_{t,H}=c_{H}+a_{H}\sigma_{t}^{2}+\frac{1}{H}\int_{0}^{H}\epsilon_{t,s}^{\sigma}ds,\label{eq:int_vol_equation}
\end{equation}

\end_inset

with
\begin_inset Formula 
\[
a_{H}=\frac{1}{H}\int_{0}^{H}A_{s}ds=\frac{1}{\kappa H}\left(1-A_{H}\right),\quad c_{H}=\mu\left(1-a_{H}\right).
\]

\end_inset

The error term may be simplified:
\begin_inset Formula 
\[
\begin{aligned}\frac{1}{H}\int_{0}^{H}\epsilon_{t,s}^{\sigma}ds= & \frac{1}{H}\int_{0}^{H}\left(\eta\int_{t}^{t+s}\sigma_{u}A_{t+s-u}dW_{u}^{\sigma}\right)ds\\
= & \frac{\eta}{H}\int_{t}^{t+H}\sigma_{u}\left(\int_{u-t}^{H}A_{t+s-u}ds\right)dW_{u}^{\sigma}\\
= & \frac{\eta}{H}\int_{t}^{t+H}\sigma_{u}\left(\int_{0}^{t+H-u}A_{s}ds\right)dW_{u}^{\sigma}\\
= & \eta\int_{t}^{t+H}\sigma_{u}a_{t+H-u}dW_{u}^{\sigma}.
\end{aligned}
\]

\end_inset

From this we can guess that
\begin_inset Formula 
\[
E_{t}\left[\left(1-A_{h}L\right)\left(1-A_{h}^{2}L\right)\mathcal{V}_{t+2h,H}^{2}\right]=C_{2}=\left(1-A_{h}\right)\left(1-A_{h}^{2}\right)E\left[\mathcal{V}_{t,H}^{2}\right].
\]

\end_inset

From the equation 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:int_vol_equation"

\end_inset

 for 
\begin_inset Formula $\mathcal{V}_{t,H}$
\end_inset

 we can derive
\begin_inset Formula 
\[
\begin{aligned}V\left[\mathcal{V}_{t,H}\right]= & a_{H}^{2}V\left[\sigma_{t}^{2}\right]+V\left[\frac{1}{H}\int_{0}^{H}\epsilon_{t,s}^{\sigma}ds\right]=a_{H}^{2}\frac{\mu\eta^{2}}{2\kappa}+V\left[\eta\int_{t}^{t+H}\sigma_{u}a_{t+H-u}dW_{u}^{\sigma}\right]\\
= & a_{H}^{2}\frac{\mu\eta^{2}}{2\kappa}+E\left[\eta^{2}\int_{t}^{t+H}\sigma_{u}^{2}a_{t+H-u}^{2}du\right]=a_{H}^{2}\frac{\mu\eta^{2}}{2\kappa}+\mu\eta^{2}\int_{0}^{H}a_{H-u}^{2}du\\
= & a_{H}^{2}\frac{\mu\eta^{2}}{2\kappa}+\frac{\mu\eta^{2}}{\kappa^{2}H^{2}}\int_{0}^{H}\left(1-A_{H-u}\right)^{2}du\\
= & \left(1-A_{H}\right)^{2}\frac{\mu\eta^{2}}{2\kappa^{3}H^{2}}+\frac{\mu\eta^{2}}{\kappa^{2}H^{2}}\left(H-\frac{2}{\kappa}\left(1-A_{H}\right)+\frac{1}{2\kappa}\left(1-A_{2H}\right)\right)\\
= & \frac{\mu\eta^{2}}{\kappa^{2}H^{2}}\left[\left(1-2A_{H}+A_{2H}\right)\frac{1}{2\kappa}+H-\frac{2}{\kappa}\left(1-A_{H}\right)+\frac{1}{2\kappa}\left(1-A_{2H}\right)\right]\\
= & \frac{\mu\eta^{2}}{\kappa^{2}H^{2}}\left[\left(1-A_{H}\right)\frac{1}{\kappa}+H-\frac{2}{\kappa}\left(1-A_{H}\right)\right]\\
= & \frac{\mu\eta^{2}}{\kappa^{2}H^{2}}\left[H-\frac{1}{\kappa}\left(1-A_{H}\right)\right]=\frac{\mu\eta^{2}}{\kappa^{2}H}\left(1-a_{H}\right)=\frac{\eta^{2}}{\kappa^{2}}\frac{c_{H}}{H}.
\end{aligned}
\]

\end_inset

Hence,
\begin_inset Formula 
\[
E\left[\mathcal{V}_{t,H}^{2}\right]=V\left[\mathcal{V}_{t,H}\right]+\left(E\left[\mathcal{V}_{t,H}\right]\right)^{2}=\frac{\eta^{2}}{\kappa^{2}}\frac{c_{H}}{H}+\mu^{2}.
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Note Comment
status open

\begin_layout Plain Layout
Taking conditional expectation of the integrated equation we obtain
\begin_inset Formula 
\[
E_{t}\left[\mathcal{V}_{t,h}\right]=c_{h}+a_{h}\sigma_{t}^{2}.
\]

\end_inset

The conditional variance of integrated volatility is
\begin_inset Formula 
\[
\begin{aligned}V_{t}\left[\mathcal{V}_{t,h}\right]= & V_{t}\left[\frac{1}{h}\int_{0}^{h}\epsilon_{t,s}^{\sigma}ds\right]\\
= & \frac{\eta^{2}}{h^{2}}\int_{t}^{t+h}E_{t}\left[\sigma_{u}^{2}\right]a_{t+h-u}^{2}du\\
= & \frac{\eta^{2}}{h^{2}}\int_{t}^{t+h}\left(A_{u-t}\sigma_{t}^{2}+C_{u-t}\right)a_{t+h-u}^{2}du\\
= & \sigma_{t}^{2}\frac{\eta^{2}}{h^{2}}\int_{0}^{h}A_{u}a_{h-u}^{2}du+\frac{\eta^{2}}{h^{2}}\int_{0}^{h}C_{u}a_{h-u}^{2}du\\
= & D_{2}\sigma_{t}^{2}+F_{2}.
\end{aligned}
\]

\end_inset

with
\begin_inset Formula 
\[
\begin{aligned}D_{2}= & \eta^{2}\int_{0}^{h}A_{u}a_{h-u}^{2}du\\
= & \frac{\eta^{2}}{\kappa^{2}h^{2}}\int_{0}^{h}\left(A_{u}-2A_{h}+A_{2h-u}\right)du\\
= & \frac{\eta^{2}}{\kappa^{2}h^{2}}\left(\frac{1}{\kappa}\left(1-A_{h}\right)-2A_{h}h+\frac{1}{\kappa}A_{h}\left(1-A_{h}\right)\right)\\
= & \frac{\eta^{2}}{\kappa^{2}h^{2}}\left(\frac{1}{\kappa}\left(1-A_{2h}\right)-2A_{h}h\right),
\end{aligned}
\]

\end_inset

and
\begin_inset Formula 
\[
\begin{aligned}F_{2}= & \frac{\eta^{2}}{h^{2}}\int_{0}^{h}C_{u}a_{h-u}^{2}du\\
= & \frac{\mu\eta^{2}}{\kappa^{2}h^{2}}\int_{0}^{h}\left(1-A_{u}\right)\left(1-2A_{h-u}+A_{2h-2u}\right)du\\
= & \frac{\mu\eta^{2}}{\kappa^{2}h^{2}}\int_{0}^{h}\left(1-A_{u}-2A_{h-u}+2A_{h}+A_{2h-2u}-A_{2h-u}\right)du\\
= & \frac{\mu\eta^{2}}{\kappa^{2}h^{2}}\left(h-\frac{1}{\kappa}\left(1-A_{h}\right)-\frac{2}{\kappa}\left(1-A_{h}\right)+2A_{h}h+\frac{1}{2\kappa}\left(1-A_{2h}\right)-\frac{1}{\kappa}A_{h}\left(1-A_{h}\right)\right)\\
= & \frac{\mu\eta^{2}}{\kappa^{2}h^{2}}\left(h\left(1+2A_{h}\right)-\frac{1}{2\kappa}\left(1-A_{h}\right)\left(5+A_{h}\right)\right).
\end{aligned}
\]

\end_inset

Hence, the second moment is
\begin_inset Formula 
\[
E_{t}\left[\mathcal{V}_{t,h}^{2}\right]=F_{2}+\frac{c_{h}^{2}}{h^{2}}+\left(D_{2}+\frac{2a_{h}c_{h}}{h^{2}}\right)\sigma_{t}^{2}+\frac{a_{h}^{2}}{h^{2}}\sigma_{t}^{4}.
\]

\end_inset

Multiply this by 
\begin_inset Formula $\left(1-A_{h}L\right)\left(1-A_{h}^{2}L\right)$
\end_inset

 and use previously derived conditional moments to obtain
\begin_inset Formula 
\[
\begin{aligned}E_{t}\left[\left(1-A_{h}L\right)\left(1-A_{h}^{2}L\right)\mathcal{V}_{t+2h,h}^{2}\right]= & \left(1-A_{h}\right)\left(1-A_{h}^{2}\right)\left(F_{2}+\frac{c_{h}^{2}}{h^{2}}\right)+\left(D_{2}+\frac{2a_{h}c_{h}}{h^{2}}\right)\left(1-A_{h}^{2}\right)C_{h}+\frac{a_{h}^{2}}{h^{2}}R_{1}\\
\equiv & R_{2}.
\end{aligned}
\]

\end_inset


\end_layout

\end_inset


\end_layout

\begin_layout Standard
The return equation can be integrated on 
\begin_inset Formula $\left[t,t+H\right]$
\end_inset

 to obtain
\begin_inset Formula 
\[
\tilde{r}_{t,h}\equiv\frac{1}{H}\int_{t}^{t+H}dp_{s}-r=\left(\lambda-\frac{1}{2}\right)\mathcal{V}_{t,H}+\frac{1}{H}\int_{t}^{t+H}\sigma_{s}dW_{s}^{r},
\]

\end_inset

The return first conditional moment is
\begin_inset Formula 
\[
E_{t}\left[\tilde{r}_{t,H}\right]=\left(\lambda-\frac{1}{2}\right)E_{t}\left[\mathcal{V}_{t,H}\right],
\]

\end_inset

